								<Column id="Documento" minScreenWidth="Tablet" demandPopin="true">
									<Text text="{i18n>tableNameDocumentoTitle}"/>
								</Column>
							</columns>
							<items>
								<ColumnListItem>
									<cells>
										<Text text="{modelPath>Num}"/>
										<ObjectIdentifier title="{modelPath>Ticket}"/>
										<Text text="{modelPath>Bukrs}"/>
										<Text text="{modelPath>Codtorre}"/>
										<Text text="{modelPath>Destorre}"/>
										<Text text="{modelPath>Codproc}"/>
										<Text text="{modelPath>Desproc}"/>
										<Text text="{modelPath>Statustk}"/>
										<ObjectNumber number="{modelPath>Desstattk}" state="{ path: 'modelPath>Statustk', formatter: '.formatter.status' }"/>
										<Text text="{modelPath>Usercrea}"/>
										<Text text="{path: 'modelPath>Credatetk', formatter: '.formatter.formatDate'}"/>
										<Text text="{path: 'modelPath>Cretimetk', formatter: '.formatter.formatTime'}"/>
										<Text text="{path: 'modelPath>Upddatetk', formatter: '.formatter.formatDate'}"/>
										<Text text="{path: 'modelPath>Updtimetk', formatter: '.formatter.formatTime'}"/>
										<Text text="{modelPath>Despostip}"/>
										<Text text="{modelPath>Tipp}"/>
										<Text text="{modelPath>Pasoproc}"/>
										<Text text="{modelPath>Txtpaso}"/>
										<Text text="{modelPath>Status}"/>
										<ObjectNumber number="{modelPath>Desstat}" state="{ path: 'modelPath>Status', formatter: '.formatter.status' }"/>
										<Text text="{modelPath>Codtorrehijo}"/>
										<Text text="{modelPath>Destorrehijo}"/>
										<Text text="{modelPath>Codprochijo}"/>
										<Text text="{modelPath>Desprochijo}"/>
										<Text text="{modelPath>Opcbifur}"/>
										<Text text="{modelPath>Txopbif}"/>
										<Text text="{modelPath>Cargo}"/>
										<Text text="{modelPath>Descargo}"/>
										<Text text="{modelPath>Creuser}"/>
										<Text text="{modelPath>Nomuser}"/>
										<Text text="{path: 'modelPath>Zdate', formatter: '.formatter.formatDate'}"/>
										<Text text="{path: 'modelPath>Time', formatter: '.formatter.formatTime'}"/>
										<Text text="{path: 'modelPath>Upddate', formatter: '.formatter.formatDate'}"/>
										<Text text="{path: 'modelPath>Updtime', formatter: '.formatter.formatTime'}"/>
										<Text text="{modelPath>Tipcomp}"/>
										<Text text="{modelPath>Waers}"/>
										<Text text="{modelPath>Valcomp}"/>
										<Text text="{modelPath>Documento}"/>
									</cells>
								</ColumnListItem>
							</items>
						</Table>
					</ScrollContainer>
				</content>
			</IconTabBar>
		</semantic:content>
		<semantic:sendEmailAction>
			<semantic:SendEmailAction id="shareEmail" press="onShareEmailPress"/>
		</semantic:sendEmailAction>
		<semantic:shareInJamAction>
			<semantic:ShareInJamAction id="shareInJam" visible="{FLP>/isShareInJamActive}" press="onShareInJamPress"/>
		</semantic:shareInJamAction>
		<semantic:saveAsTileAction>
			<footerbar:AddBookmarkButton id="shareTile" title="{worklistView>/saveAsTileTitle}"/>
		</semantic:saveAsTileAction>
	</semantic:SemanticPage>
